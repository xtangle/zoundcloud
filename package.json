{
  "name": "zoundcloud",
  "version": "1.3.1",
  "private": true,
  "description": "Adds download buttons to SoundCloud pages.",
  "main": "index.js",
  "engines": {
    "node": ">=7.6"
  },
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:watch": "webpack --config webpack.dev.js --watch",
    "ci": "run-s -c test:coverage:ci build build:prod",
    "coveralls": "cat coverage/HeadlessChrome/lcovonly | coveralls",
    "lint": "tslint -p .",
    "precommit": "run-s test:coverage build build:prod",
    "start": "yarn run build:watch",
    "test": "karma start karma.conf.js",
    "test:coverage": "karma start karma.conf.js --reporters mocha,karma-typescript",
    "test:coverage:ci": "run-s test:coverage coveralls",
    "test:stability": "run-s test test test test test",
    "test:watch": "karma start karma.conf.js --single-run false --mochaReporter.output autowatch"
  },
  "pre-commit": "precommit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtangle/ZoundCloud.git"
  },
  "author": "xtangle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xtangle/ZoundCloud/issues"
  },
  "homepage": "https://github.com/xtangle/ZoundCloud#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/chrome": "^0.0.74",
    "@types/jquery": "^3.3.11",
    "@types/mocha": "^5.2.5",
    "@types/mocha-each": "^1.1.0",
    "@types/node": "^10.11.4",
    "@types/sinon": "^5.0.4",
    "@types/sinon-chai": "^3.2.0",
    "@types/sinon-chrome": "^2.2.4",
    "@types/verror": "^1.10.3",
    "browser-id3-writer": "^4.1.0",
    "chai": "^4.2.0",
    "chai-jq": "^0.0.9",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "coveralls": "^3.0.2",
    "fork-ts-checker-webpack-plugin": "^0.4.10",
    "jquery": "^3.3.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^3.0.13",
    "mocha": "^5.2.0",
    "mocha-each": "^1.1.0",
    "npm-run-all": "^4.1.3",
    "pre-commit": "^1.2.2",
    "rxjs": "^6.3.3",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "sinon-chrome": "^2.3.2",
    "terser-webpack-plugin": "^1.1.0",
    "ts-loader": "^5.2.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.1.1",
    "verror": "^1.10.0",
    "webpack": "^4.20.2",
    "webpack-chrome-extension-reloader": "^0.8.3",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
    "yargs": "^12.0.2"
  }
}
