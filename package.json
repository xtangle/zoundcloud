{
  "name": "zoundcloud",
  "version": "1.3.1",
  "private": true,
  "description": "Adds download buttons to SoundCloud pages.",
  "main": "index.js",
  "engines": {
    "node": ">=7.6"
  },
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:watch": "webpack --config webpack.dev.js --watch",
    "ci": "run-s test:coverage coveralls build build:prod e2e",
    "coveralls": "cat coverage/HeadlessChrome/lcovonly | coveralls",
    "e2e": "nightwatch",
    "lint": "tslint -p .",
    "precommit": "run-s test:coverage build build:prod",
    "start": "yarn run build:watch",
    "test": "karma start karma.conf.js",
    "test:coverage": "karma start karma.conf.js --reporters mocha,karma-typescript",
    "test:watch": "karma start karma.conf.js --single-run false --mochaReporter.output autowatch"
  },
  "pre-commit": "precommit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtangle/ZoundCloud.git"
  },
  "author": "xtangle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xtangle/ZoundCloud/issues"
  },
  "homepage": "https://github.com/xtangle/ZoundCloud#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chrome": "^0.0.69",
    "@types/jquery": "^3.3.4",
    "@types/mocha": "^5.2.3",
    "@types/mocha-each": "^1.1.0",
    "@types/node": "^10.3.5",
    "@types/sinon": "^5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "@types/sinon-chrome": "^2.2.4",
    "@types/verror": "^1.10.3",
    "browser-id3-writer": "^4.0.0",
    "chai": "^4.1.2",
    "chai-jq": "^0.0.9",
    "chromedriver": "^2.42.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.4.2",
    "jquery": "^3.3.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^3.0.12",
    "mocha": "^5.2.0",
    "mocha-each": "^1.1.0",
    "nightwatch": "^0.9.21",
    "npm-run-all": "^4.1.3",
    "pre-commit": "^1.2.2",
    "rxjs": "^6.2.1",
    "sinon": "^6.0.0",
    "sinon-chai": "^3.2.0",
    "sinon-chrome": "^2.3.2",
    "ts-loader": "^4.4.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "verror": "^1.10.0",
    "webpack": "^4.12.0",
    "webpack-chrome-extension-reloader": "^0.8.3",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.3",
    "yargs": "^11.0.0"
  }
}
