{
  "name": "ZoundCloud Downloader",
  "short_name": "ZoundCloud",
  "description": "Adds download buttons to SoundCloud pages.",
  "version": "1.4.4",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "downloads",
    "storage",
    "tabs",
    "webNavigation",
    "*://soundcloud.com/*",
    "*://api.soundcloud.com/*"
  ],
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "vendor.js",
      "background.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "page_action": {}
}
