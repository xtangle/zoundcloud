{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "arrow-return-shorthand": [
      true,
      "multiline"
    ],
    "indent": [
      true,
      "spaces"
    ],
    "max-line-length": [
      true,
      120
    ],
    "no-relative-imports": true,
    "no-unused-expression": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ],
    "ter-indent": [
      true,
      2,
      {
        "SwitchCase": 1
      }
    ]
  },
  "rulesDirectory": [
    "node_modules/tslint-microsoft-contrib"
  ]
}
